{
    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.default.compileCommands": "${workspaceFolder}/build/compile_commands.json",
    "C_Cpp.intelliSenseEngine": "disabled",
    "C_Cpp.formatting": "clangFormat",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.fontSize": 16,
    "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace",
    "editor.mouseWheelZoom": true,
    "editor.inlayHints.fontSize": 13,
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd",
    "editor.smoothScrolling": false,
    "editor.mouseWheelScrollSensitivity": 4,
    "editor.formatOnSave": false,
    "window.zoomLevel": 0,
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.middleClickBehavior": "default",
    "debug.console.closeOnEnd": true,
    "explorer.autoReveal": "focusNoScroll",
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "cmake.parallelJobs": 10,
    "cmake.automaticReconfigure": false,
    "cmake.exportCompileCommandsFile": true,
    "cmake.configureOnOpen": false,
    "cmake.options.statusBarVisibility": "compact",
    "cmake.generator": "Ninja",
    "cmake.ctest.parallelJobs": 8,
    "cmake.configureArgs": [
        "-DSKL_TUNE_PRESET=qa",
        "-DSKL_BUILD_TYPE=DEV"
    ],
    "cmake.ctestArgs": [
        "--verbose"
    ],
    "cmake.testEnvironment": {
        "LSAN_OPTIONS": "verbosity=1:log_threads=1",
        "ASAN_OPTIONS": "log_path=${workspaceFolder}/build/asan_logs/asan_log"
    },
    "workbench.editor.mouseBackForwardToNavigate": true,
    "workbench.statusBar.visible": true,
    "gtestExplorer.cwd": "${workspaceFolder}/build/all_skl_tests",
    "[cpp]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "[c]": {
        "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
    },
    "[cmake]": {
        "editor.defaultFormatter": "KylinIdeTeam.cmake-intellisence",
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.detectIndentation": false
    },
    "clangd.path": "clangd-20",
    "clangd.arguments": [
        "-j",
        "4",
        "--background-index",
        "--pch-storage",
        "memory",
        "--log",
        "error",
        "--header-insertion=never",
        "--compile-commands-dir",
        "${workspaceFolder}/build/",
    ],
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "mesonbuild.configureOnOpen": false,
    "codium.codeCompletion.enable": true,
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
